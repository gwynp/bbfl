<h1> BBFL Franchise List</h1>


<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Franchise</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.franchise %></td>
        <td><%= user.email %></td>
       
            <td><%= link_to 'Change', edit_user_registration_path(user) %></td>
            <td><%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>